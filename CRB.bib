  author = {Puker, A and Rodrigues, S R and Tiago, E F and Ide, S and Fuhrmann, J},
  year = {2011},
  keywords = {Oryctes rhinoceros,Coleoptera,Scarabaeidae,adult,allometry,beetle,behavioral ecology,Brazil,color,crop pest,crop plant,Dynastinae,ecotone,laboratory method,longevity,Mato Grosso do Sul,Oryctini,oviposition,Pirapo River,Pleurosticti,sexual selection,turnover},
