@article{kinawy_m.m._control_1987,
  title = {Control of Coconut Rhinoceros Beetle {{Oryctes Rhinoceros L}}.({{Scarabidae}}: {{Coleoptera}}) in {{South Oman}}},
  volume = {FAO (Ministry of Agriculture, Cairo (Egypt). Plant Protection Research Inst.)},
  abstract = {Infestation of coconut palm (Cocos nucifera L.) with coconut rhinoceros beetle (Oryctes rhinoceros L.) at five different localities in south Oman namely, Augad and El-Wadi, Salalah, El-Hafa and El-Karad, El-Dahariz and Tagah was approximated during 1983-1986. Infestation increased from 30\% in 1983 to 68\% in 1986. An attempt to control that pest mechanically and chemically was carried out in 1985 and 1986. A total of 6573 beetles could be extracted from 58630 infested trees that were treated with BHC at a concentration of (6.5\%). As a result of such treatment, the mean number of beetles/100 trees decreased considerably at three localities; Augad and El-Wadi, El-Dahariz and Tagah. Meanwhile, beetles population increased at Salalah and El-Hafa and El-Karad localities. Reduction of mean percent of infestation in 1985 and 1986 ranged 77-100\% according to locality.},
