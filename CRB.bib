  title = {{{MORPHOMETRIC ANALYSIS OF ORYCTES RHINOCEROS}} ({{L}}.) ({{COLEOPTERA}}: {{SCARABAEIDAE}}) {{FROM OIL PALM PLANTATIONS}}},
